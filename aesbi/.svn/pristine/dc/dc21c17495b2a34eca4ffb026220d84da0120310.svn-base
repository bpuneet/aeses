(SELECT TIMESTAMP FROM bi_dim_date WHERE id = (SELECT MAX( bi_dim_date_id) FROM bi_fact_markets_part
WHERE bi_dim_market_id  IN (1096,1097,1098,1099,1100,1101,1102,1103,1104,1105,1106,1107,1108,1109,1110)))) c
