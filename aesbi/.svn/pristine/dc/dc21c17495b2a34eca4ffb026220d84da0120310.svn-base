(SELECT TIMESTAMP FROM bi_dim_date WHERE id = (SELECT MAX( bi_dim_date_id) FROM bi_fact_markets_part
WHERE bi_dim_market_id  IN (111,1112,1113,1114,1115,1116,1117,1118,1119,1120,1121,1122,1123,1124,1125)))) c
