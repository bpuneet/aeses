        <class_source><![CDATA[import java.io.FileOutputStream;
import java.io.InputStream;
import com.google.gdata.client.spreadsheet.SpreadsheetService;
import com.google.gdata.client.GoogleAuthTokenFactory.UserToken;
import com.google.gdata.client.docs.*;
import com.google.gdata.data.MediaContent;
import com.google.gdata.data.media.MediaSource;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] r = getRow();
	  DocsService docService = new DocsService("");

	if (r == null) {
		setOutputDone();
		return false;
	}

    if (first){
        first = false;
    }
	
try{
	String resourceId = "spreadsheet:0Ak45fukExJ4ZdHRfcmlkZ3Zzc1VTQnhIeDJNaWpOWHc";
        SpreadsheetService spread_client = new SpreadsheetService("");
        spread_client.setUserCredentials("aescrowd@gmail.com", "aescrowd123");

        // Substitute the spreadsheets token for the docs token
        UserToken spreadsheetsToken1 = (UserToken) spread_client.getAuthTokenFactory().getAuthToken();
        docService.setUserToken(spreadsheetsToken1.getValue());

   
        String key = resourceId.substring(resourceId.lastIndexOf(':') + 1);

        String exportUrl = "https://spreadsheets.google.com/feeds/download/spreadsheets"
            + "/Export?key=" + key + "&fmcmd=4";

   
        System.out.println("Exporting document from: " + exportUrl);

        MediaContent mc = new MediaContent();
        mc.setUri(exportUrl.toString());
        MediaSource ms = docService.getMedia(mc);

        InputStream inStream = null;
        FileOutputStream outStream = null;

        try {
            inStream = ms.getInputStream();
            outStream = new FileOutputStream("/usr/local/data/bi_data/ETL/Google_docs/LM_RunSchedule.xlsx");


            int c;
            while ((c = inStream.read()) != -1) {
                outStream.write(c);
            }
        } finally {
            if (inStream != null) {
                inStream.close();
            }

            if (outStream != null) {
                outStream.flush();
                outStream.close();
            }
        }
	}

	catch(Exception e){ e.getMessage();}

	return true;
}]]></class_source>
