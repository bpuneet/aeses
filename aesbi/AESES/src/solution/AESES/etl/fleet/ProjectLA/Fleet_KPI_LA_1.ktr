    <connection>scada</connection>
    <sql>SELECT i.date AS &apos;date&apos;,
    la.energy_throughput AS &quot;energy_throughput&quot;,
    la.disposition AS &quot;disposition&quot;,
    la.charge_crate AS &quot;charge_crate&quot;,
    la.discharge_crate AS &quot;discharge_crate&quot;,
    la.c_rate AS  &quot;c_rate&quot;,
      
    NULL AS Signal_Intensity,
    NULL AS Energy_Efficiency,
    NULL AS Roundtrip_Energy,
    NULL AS revenue_capture,
    NULL AS potential_revenue,
    NULL AS gross_revenue,
    NULL AS other_charge,
    NULL AS energy_cost,
    NULL AS energy_losses,
    NULL AS market_capture,
    NULL AS planned_outage_rate,
    NULL AS unplanned_outage_rate,
    NULL AS &quot;availability_factor&quot;,
    (i.total_gross_power_capacity &#47; 3200000 ) * 100 AS &quot;Available_Capacity&quot;,
   0 AS &quot;jira_id&quot;,
    ? as &quot;maxdate&quot;   
FROM
    
(SELECT x.date AS TIMESTAMP, 
ROUND((x.output*8760&#47;(y.total_gross_power_capacity*x.countP)),4) AS energy_throughput,

   ROUND((x.dispositionvalue*1000 &#47; y.total_gross_power_capacity),4) AS disposition,
   ROUND(h.charge_nyseg_plc_ovation_gross_kw&#47;(y.total_gross_power_capacity&#47;4),4) AS &quot;charge_crate&quot;,
   ROUND(h.discharge_nyseg_plc_ovation_gross_kw&#47;(y.total_gross_power_capacity&#47;4),4) AS &quot;discharge_crate&quot;,
   ROUND(h.nyseg_plc_ovation_gross_kw&#47;(y.total_gross_power_capacity&#47;4),4) AS &quot;c_rate&quot;,
DATE(x.date) AS DATE,HOUR(x.date) AS hr,FLOOR(MINUTE(x.date)&#47;15) AS minutes

FROM

(SELECT FROM_UNIXTIME(ts&#47;1000) AS date,
   ABS(pointValue) AS &quot;output&quot;,
   COUNT(pointValue) AS countP,
   AVG(pointValue) AS &quot;dispositionvalue&quot;
    
            FROM mango.pointValues 
WHERE ts   BETWEEN UNIX_TIMESTAMP(CONVERT_TZ(?,&apos;US&#47;Eastern&apos;,&apos;GMT&apos;))* 1000 
         AND UNIX_TIMESTAMP(CONVERT_TZ(?,&apos;US&#47;Eastern&apos;,&apos;GMT&apos;))* 1000 AND dataPointId = 932
GROUP BY YEAR(FROM_UNIXTIME(ts&#47;1000)),DAYOFYEAR(FROM_UNIXTIME(ts&#47;1000)),HOUR(FROM_UNIXTIME(ts&#47;1000)),MINUTE(FROM_UNIXTIME(ts&#47;1000)),
FLOOR(SECOND(FROM_UNIXTIME(ts&#47;1000))&#47;10)) `x`

JOIN

(SELECT AVG(pointValue) AS total_gross_power_capacity, FROM_UNIXTIME(ts&#47;1000) AS DATE
    FROM mango.pointValues
    WHERE dataPointId = 6180 AND
       ts  BETWEEN UNIX_TIMESTAMP(CONVERT_TZ(?,&apos;US&#47;Eastern&apos;,&apos;GMT&apos;))* 1000 
         AND UNIX_TIMESTAMP(CONVERT_TZ(?,&apos;US&#47;Eastern&apos;,&apos;GMT&apos;))* 1000
    GROUP BY YEAR(FROM_UNIXTIME(ts&#47;1000)),DAYOFYEAR(FROM_UNIXTIME(ts&#47;1000)),
    HOUR(FROM_UNIXTIME(ts&#47;1000)),MINUTE(FROM_UNIXTIME(ts&#47;1000)) ,FLOOR(SECOND(FROM_UNIXTIME(ts&#47;1000))&#47;10)) `y`
    ON x.date = y.date
    
    JOIN
    
 (SELECT  
AVG(IF(pointValue &lt; 0,ABS(pointValue),NULL)) AS charge_nyseg_plc_ovation_gross_kw, 
AVG(IF(pointValue &gt;= 0,ABS(pointValue),NULL)) AS discharge_nyseg_plc_ovation_gross_kw, 

AVG(ABS(PointValue)) AS nyseg_plc_ovation_gross_kw, FROM_UNIXTIME(ts&#47;1000) AS DATE
    FROM mango.pointValues
    WHERE dataPointId = 932 AND
        ts  BETWEEN UNIX_TIMESTAMP(CONVERT_TZ(?,&apos;US&#47;Eastern&apos;,&apos;GMT&apos;))* 1000 
         AND UNIX_TIMESTAMP(CONVERT_TZ(?,&apos;US&#47;Eastern&apos;,&apos;GMT&apos;))* 1000
      GROUP BY YEAR(FROM_UNIXTIME(ts&#47;1000)),DAYOFYEAR(FROM_UNIXTIME(ts&#47;1000)),
    HOUR(FROM_UNIXTIME(ts&#47;1000)),MINUTE(FROM_UNIXTIME(ts&#47;1000)) ,FLOOR(SECOND(FROM_UNIXTIME(ts&#47;1000))&#47;10)
  ) h
    ON y.date = h.date
       
 GROUP BY
YEAR(y.date),
DAYOFYEAR(y.date),HOUR(y.date),FLOOR(MINUTE(y.date)&#47;15))la

INNER JOIN

(SELECT AVG(pointValue) AS total_gross_power_capacity, FROM_UNIXTIME(ts&#47;1000) AS DATE
    FROM mango.pointValues
    WHERE dataPointId = 6180 AND
       ts   BETWEEN UNIX_TIMESTAMP(CONVERT_TZ(?,&apos;US&#47;Eastern&apos;,&apos;GMT&apos;))* 1000 
         AND UNIX_TIMESTAMP(CONVERT_TZ(?,&apos;US&#47;Eastern&apos;,&apos;GMT&apos;))* 1000
    GROUP BY YEAR(FROM_UNIXTIME(ts&#47;1000)),DAYOFYEAR(FROM_UNIXTIME(ts&#47;1000)),
    HOUR(FROM_UNIXTIME(ts&#47;1000)),FLOOR(MINUTE(FROM_UNIXTIME(ts&#47;1000))&#47;15)) i
    ON la.timestamp=i.date
    
GROUP BY
YEAR(i.date),
DAYOFYEAR(i.date),HOUR(i.date),FLOOR(MINUTE(i.date)&#47;15)



 </sql>
    <limit>0</limit>
    <lookup>format</lookup>
