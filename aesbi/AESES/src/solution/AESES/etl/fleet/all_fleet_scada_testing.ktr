(SELECT FROM_UNIXTIME(Max(ts)&#47;1000) as &quot;Laurel_Mountain_maxdate&quot;
FROM mango.pointValues  
WHERE  dataPointId = 5366)c

join
(SELECT FROM_UNIXTIME(Max(ts)&#47;1000) as &quot;Angamos_maxdate&quot;
FROM mango.pointValues  
WHERE  dataPointId = 5874)d</sql>
