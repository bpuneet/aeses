    <connection>aespro</connection>
    <sql>SELECT b.timestamp,
    b.id bi_dim_date_id,
    c.id bi_dim_market_id,
    d.id bi_dim_fleet_id,
    e.id bi_dim_es_service_id,
    f.id bi_dim_supplier_id,
    g.id bi_dim_technology_id,
    h.id as jira_id
    
           
FROM

(SELECT * FROM bi_dim_date WHERE TIMESTAMP_gmt BETWEEN &apos;2011-01-01 05:00:00&apos;
AND date_add(curdate(),interval 1 day)) b
    
JOIN
   
(SELECT * FROM bi_dim_market WHERE  iso = &apos;PJM&apos; AND market = &apos;Day-Ahead Regulation&apos; AND service = &apos;Regulation&apos; AND zone = &apos;ALL&apos;) c
JOIN
(SELECT * FROM bi_dim_fleet WHERE id=500) d
 JOIN
(SELECT * FROM bi_dim_es_service WHERE es_service = &apos;Frequency Regulation&apos;)e
 JOIN
(SELECT * FROM bi_dim_supplier WHERE supplier = &apos;A123&apos;)f
 JOIN
(SELECT * FROM bi_dim_technology WHERE technology =&apos;Advanced Lithium-Ion Battery&apos;) g
JOIN
(select * from bi_dim_jira where id = 301)h
 GROUP BY
YEAR(b.TIMESTAMP_gmt),
DAYOFYEAR(b.TIMESTAMP_gmt),HOUR(b.TIMESTAMP_gmt),FLOOR(MINUTE(b.TIMESTAMP_gmt)&#47;15)</sql>
