  <step>
    <name>SCADA KPIs</name>
    <type>TableInput</type>
    <description/>
    <distribute>N</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>scada</connection>
    <sql>SELECT i.date as &apos;date&apos;,
    ROUND(((8760 * a.energy &#47; i.total_gross_power_capacity) &#47; (60*60&#47;4) &#47; ((15*60&#47;4) &#47; (60*60&#47;4)) *1000)&#47;1000,4) AS energy_throughput,
    ROUND((a.dis &#47; i.total_gross_power_capacity) &#47; (15*60&#47;4) * 1000,4) AS disposition,
    h.date,
    ROUND(h.charge_nyseg_plc_ovation_gross_kw&#47;(i.total_gross_power_capacity&#47;4),4) AS &quot;charge_crate&quot;,
    ROUND(h.discharge_nyseg_plc_ovation_gross_kw&#47;(i.total_gross_power_capacity&#47;4),4) AS &quot;discharge_crate&quot;,
    ROUND(h.nyseg_plc_ovation_gross_kw&#47;(i.total_gross_power_capacity&#47;4),4) AS &quot;c_rate&quot;,
    z.nyseg_ads_signal as Signal_Intensity,
   (i.total_gross_power_capacity &#47; 3200000) * 100 as &quot;Available_Capacity&quot;,
    0 as &quot;jira_id&quot;,
    ? as &quot;maxdate&quot;
   
           
FROM
(SELECT AVG(IF(pointValue &lt; 0,ABS(pointValue),NULL)) AS charge_nyseg_plc_ovation_gross_kw, 
AVG(IF(pointValue &gt;= 0,ABS(pointValue),NULL)) AS discharge_nyseg_plc_ovation_gross_kw, 
avg(abs(PointValue)) AS nyseg_plc_ovation_gross_kw, FROM_UNIXTIME(ts&#47;1000) AS DATE
    FROM mango.pointValues
    WHERE dataPointId = 5366 AND
        ts  BETWEEN UNIX_TIMESTAMP(CONVERT_TZ(?,&apos;US&#47;Eastern&apos;,&apos;GMT&apos;))* 1000 
         AND UNIX_TIMESTAMP(CONVERT_TZ(?,&apos;US&#47;Eastern&apos;,&apos;GMT&apos;))* 1000
    GROUP BY YEAR(FROM_UNIXTIME(ts&#47;1000)),DAYOFYEAR(FROM_UNIXTIME(ts&#47;1000)),
    HOUR(FROM_UNIXTIME(ts&#47;1000)),FLOOR(minute(FROM_UNIXTIME(ts&#47;1000))&#47;15)) h
JOIN
(SELECT avg(pointValue) AS total_gross_power_capacity, FROM_UNIXTIME(ts&#47;1000) AS DATE
    FROM mango.pointValues
    WHERE dataPointId = 5406 AND
       ts  BETWEEN UNIX_TIMESTAMP(CONVERT_TZ(?,&apos;US&#47;Eastern&apos;,&apos;GMT&apos;))* 1000 
         AND UNIX_TIMESTAMP(CONVERT_TZ(?,&apos;US&#47;Eastern&apos;,&apos;GMT&apos;))* 1000
    GROUP BY YEAR(FROM_UNIXTIME(ts&#47;1000)),DAYOFYEAR(FROM_UNIXTIME(ts&#47;1000)),
    HOUR(FROM_UNIXTIME(ts&#47;1000)),FLOOR(MINUTE(FROM_UNIXTIME(ts&#47;1000))&#47;15)) i
    ON h.date=i.date
JOIN
(SELECT FROM_UNIXTIME(ts&#47;1000) AS DATE,
   SUM(ABS(pointValue)) AS &quot;energy&quot;,
    SUM(pointValue) AS &quot;dis&quot;
            FROM mango.pointValues
WHERE ts  BETWEEN UNIX_TIMESTAMP(CONVERT_TZ(?,&apos;US&#47;Eastern&apos;,&apos;GMT&apos;))* 1000 
         AND UNIX_TIMESTAMP(CONVERT_TZ(?,&apos;US&#47;Eastern&apos;,&apos;GMT&apos;))* 1000 AND dataPointId = 5366
GROUP BY YEAR(FROM_UNIXTIME(ts&#47;1000)),DAYOFYEAR(FROM_UNIXTIME(ts&#47;1000)),HOUR(FROM_UNIXTIME(ts&#47;1000)),
FLOOR(MINUTE(FROM_UNIXTIME(ts&#47;1000))&#47;15)) a
    ON a.date=h.date
    JOIN
    
    (SELECT sum(pointValue) AS nyseg_ads_signal, FROM_UNIXTIME(ts&#47;1000) AS DATE
    FROM mango.pointValues
    WHERE dataPointId = 5401 AND
       ts  BETWEEN UNIX_TIMESTAMP(CONVERT_TZ(?,&apos;US&#47;Eastern&apos;,&apos;GMT&apos;))* 1000 
         AND UNIX_TIMESTAMP(CONVERT_TZ(?,&apos;US&#47;Eastern&apos;,&apos;GMT&apos;))* 1000
    GROUP BY YEAR(FROM_UNIXTIME(ts&#47;1000)),DAYOFYEAR(FROM_UNIXTIME(ts&#47;1000)),
    HOUR(FROM_UNIXTIME(ts&#47;1000)),FLOOR(MINUTE(FROM_UNIXTIME(ts&#47;1000))&#47;15))z
    ON z.date=a.date
    
 GROUP BY
YEAR(h.date),
DAYOFYEAR(h.date),HOUR(h.date),FLOOR(MINUTE(h.date)&#47;15)</sql>
    <limit>0</limit>
