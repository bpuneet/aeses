    <connection>pro</connection>
    <sql>SELECT b.id AS bi_dim_date_id,
    2010 AS bi_dim_market_id,
    a.price_mwh AS clearing_price,
    (SELECT ROUND(AVG(price_mwh),2)
    FROM  ercot_rt_energy_hb_houston_raw
    WHERE datetime_gmt BETWEEN DATE_SUB(a.datetime_gmt,INTERVAL 29 DAY) AND
    a.datetime_gmt AND HOUR(a.datetime_gmt)=HOUR(datetime_gmt)) AS thirty_day_avg,
    (SELECT ROUND(AVG(price_mwh),2)
    FROM  ercot_rt_energy_hb_houston_raw
    WHERE datetime_gmt BETWEEN DATE_SUB(a.datetime_gmt,INTERVAL 89 DAY) AND
    a.datetime_gmt AND HOUR(a.datetime_gmt)=HOUR(datetime_gmt)) AS ninety_day_avg,
    (SELECT ROUND(AVG(price_mwh),2)
    FROM  ercot_rt_energy_hb_houston_raw
    WHERE datetime_gmt=DATE_SUB(a.datetime_gmt,INTERVAL 1 YEAR)) AS previous_year_clearing_price,
    0 AS volume,
    (SELECT ROUND(AVG(price_mwh),2)
    FROM  ercot_rt_energy_hb_houston_raw
    WHERE datetime_gmt BETWEEN DATE_SUB(a.datetime_gmt,INTERVAL 1 YEAR) AND
    a.datetime_gmt AND HOUR(a.datetime_gmt)=HOUR(datetime_gmt)) AS one_year_avg
FROM
(SELECT datetime_gmt,price_mwh  FROM ercot_rt_energy_hb_houston_raw WHERE datetime_gmt&gt;=(SELECT TIMESTAMP_GMT FROM bi_dim_date WHERE id = &apos;1227265&apos;)) a
JOIN
(SELECT * FROM bi_dim_date WHERE TIMESTAMP_GMT&gt;=(SELECT TIMESTAMP_GMT FROM bi_dim_date WHERE id = &apos;1227265&apos;)) b
ON a.datetime_gmt=b.timestamp_gmt
</sql>
