    <sql>SELECT
  CURRENCY_FLAG
, TRADE_DATE
, DELIVERY_DATE
, DELIVERY_HOUR
, DELIVERY_INTERVAL
, RUN_TYPE
, ROUND(SMP,2) AS SMP
, ROUND(LAMBDA,2) AS LAMBDA
, ROUND(SYSTEM_LOAD,2) AS SYSTEM_LOAD
, CMS_TIME_STAMP
FROM Irish_EP2_stg 
WHERE currency_flag=&apos;GBP&apos; AND DELIVERY_DATE &gt;= (SELECT date(MAX(trading_period)) FROM EP2_raw)
ORDER BY DELIVERY_DATE,DELIVERY_HOUR


