    <sql>SELECT * FROM
(SELECT bi_dim_date_id  , bi_dim_market_id  FROM bi_fact_markets_part WHERE bi_dim_market_id IN (1006,1007,1008,1009,1010,1011,1012,1013,1014,1015,1016,1017,1018,1019,1020,1021,1022,1023,1024,1025,1026,1027,1028,1029,1030,1031,1032,1033,1034,1035,1036,1037,1038,1039,1040,1041,1042,1043,1044,1045,1046,1047,1048,1049,1050,1096,1097,1098,1099,1100,1101,1102,1103,1104,1105,1106,1107,1108,1109,1110,1111,1112,1113,1114,1115,1116,1117,1118,1119,1120,1121,1122,1123,1124,1125,1126,1127,1128,1129,1130,1131,1132,1133,1134,1135,1136,1137,1138,1139,1140) ORDER BY bi_dim_market_id,bi_dim_date_id) f
RIGHT OUTER JOIN
(SELECT * FROM (SELECT id FROM bi_dim_date WHERE YEAR IN (2009, 2010, 2011, 2012) AND TIMESTAMP &lt;= DATE_ADD(CURDATE(), INTERVAL 1 DAY) 
GROUP BY YEAR,MONTH,day_of_month, HOUR )a
 CROSS JOIN 
(SELECT id AS &apos;mid&apos; FROM bi_dim_market WHERE id IN (1006,1007,1008,1009,1010,1011,1012,1013,1014,1015,1016,1017,1018,1019,1020,1021,1022,1023,1024,1025,1026,1027,1028,1029,1030,1031,1032,1033,1034,1035,1036,1037,1038,1039,1040,1041,1042,1043,1044,1045,1046,1047,1048,1049,1050,1096,1097,1098,1099,1100,1101,1102,1103,1104,1105,1106,1107,1108,1109,1110,1111,1112,1113,1114,1115,1116,1117,1118,1119,1120,1121,1122,1123,1124,1125,1126,1127,1128,1129,1130,1131,1132,1133,1134,1135,1136,1137,1138,1139,1140) )b
ORDER BY MID, id)d ON f.bi_dim_date_id = d.id AND f.bi_dim_market_id = d.mid </sql>
