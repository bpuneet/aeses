          {
            "name": "cdacalculatedcolumns",
            "value": "[]",
            "type": "CdaCalculatedColumnsArray"
          },
          {
            "name": "query",
            "value": "SELECT DATE(b.timestamp) AS TIMESTAMP,\r\nAVG(a.clearing_price)AS clearing_price,\r\nAVG(a.thirty_day_avg)AS thirty_day_avg,\r\nAVG(a.ninety_day_avg)AS ninety_day_avg,\r\nAVG(a.previous_year_clearing_price)AS previous_year_clearing_price\r\nFROM (SELECT* FROM bi_fact_markets_part WHERE bi_dim_market_id in (select id from bi_dim_market where iso='NYISO' and market='Day-Ahead Energy' and service='Energy')\r\nAND bi_dim_date_id BETWEEN \r\n(SELECT id FROM bi_dim_date WHERE TIMESTAMP = date_sub(DATE_FORMAT(CURDATE(),'%Y-%m-%d 00:00:00'), interval 1 month))\r\nAND\r\n(SELECT id FROM bi_dim_date WHERE TIMESTAMP=DATE_FORMAT(CURDATE(),'%Y-%m-%d 23:45:00'))) a\r\nINNER JOIN \r\n(SELECT * FROM bi_dim_date WHERE TIMESTAMP BETWEEN date_sub(DATE_FORMAT(CURDATE(),'%Y-%m-%d 00:00:00'), interval 1 month) AND DATE_FORMAT(CURDATE(),'%Y-%m-%d 23:45:00')) b\r\n ON a.bi_dim_date_id = b.id \r\nGROUP BY DATE(b.timestamp)\r\nORDER BY  DATE(b.timestamp) ASC ",
            "type": "SqlQuery"
          },
          {
            "name": "cache",
            "value": "true",
            "type": "Boolean"
          },
          {
            "name": "cacheDuration",
            "value": 3600,
            "type": "Integer"
          }
        ]
      },
